{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.13.0",
    "generators": {
      "liber-web-gen": {
        "generatorName": "typescript-angular",
        "inputSpec": "http://localhost:8989/liber/openapi.json",
        "output": "src/app/modules/openapi",
        "additionalProperties": {
          "supportsES6": true,
          "npmVersion": "11.3.2",
          "typescriptThreePlus": true,
          "npmName": "restClient",
          "fileNaming": "kebab-case",
          "useTags": true
        }
      }
    }
  }
}
